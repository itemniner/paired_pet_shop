<%= form_tag "/shelters/#{@shelter.id}", method: :post do %>
<p><%= label_tag :title %>
<%= text_field_tag :title, @review.title %>
<p><%= label_tag :rating %>
<%= number_field_tag :rating, @review.rating %></p>
<p><%= label_tag :content %>
<%= text_field_tag :content, @review.content %></p>
<p><%= label_tag :picture %>
<%= text_field_tag :picture, @review.picture %></p>

<%= submit_tag 'Submit' %>
<% end %>
